{
  "name": "victor-hugo",
  "version": "1.0.0",
  "description": "RxMD is a Hugo boilerplate for creating truly epic websites!",
  "repository": "netlify/victor-hugo",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "start": "run-p start:**",
    "start:_webpack": "webpack-dev-server --config webpack.dev.js --hot",
    "start:blogdown": "Rscript -e 'blogdown::serve_site()'; echo 'servr has stopped'",
    "live-preview": "run-p live-preview:**",
    "live-preview:_webpack": "webpack --config webpack.dev.js -w",
    "live-preview:blogdown": "npm run start:blogdown -- ",
    "prebuild": "rimraf dist",
    "build": "npm run build:_webpack && npm run build:blogdown",
    "build:live-preview": "npm run build:_webpack && npm run build:blogdown:live-preview",
    "build:_webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "build:blogdown": "Rscript -e 'blogdown::build_site()'; echo 'done'",
    "build:blogdown:live-preview": "npm run build:blogdown -- "
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.0",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.0",
    "assets-webpack-plugin": "^3.9.7",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^3.4.1",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.18.1",
    "exports-loader": "^0.7.0",
    "fancy-log": "^1.3.2",
    "file-loader": "^4.1.0",
    "hugo-bin": "^0.52.2",
    "imports-loader": "^0.8.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "plugin-error": "^1.0.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "serialize-javascript": "^2.1.1",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "git+https://github.com/Real-Currents/uglifyjs-webpack-plugin.git#real/currents",
    "url-loader": "^2.1.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.1.4",
    "whatwg-fetch": "^3.0.0"
  },
  "resolutions": {
    "natives": "1.1.6"
  }
}
